<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context="com.bry.adcafe.ui.AdStats">

    <LinearLayout
            android:id="@+id/mainViewLayout"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/topText"
                android:layout_marginTop="1dp"
                android:textAlignment="center"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="25sp"
                android:padding="10dp"
                android:text="My Ad Stats." />

            <View
                android:layout_width="match_parent"
                android:background="@color/colorAccent"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="1dp"
                android:layout_height="1dp"/>

            <com.mindorks.placeholderview.PlaceHolderView
                android:id="@+id/PlaceHolderViewInfo"
                android:layout_width="match_parent"
                android:overScrollMode="never"
                android:layout_height="wrap_content"
                android:scrollbars="vertical"/>

            <TextView
                android:layout_width="match_parent"
                android:text="Upload an ad to see and monitor it here."
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:textSize="13sp"
                android:id="@+id/noAdsUploadedText"
                android:visibility="invisible"
                android:layout_height="wrap_content" />

</LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:gravity="center"
        android:id="@+id/droppedInternetLayoutForAdStats"
        android:visibility="gone"
        android:orientation="vertical"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="70dp"
            android:background="@drawable/ic_action_connection"
            android:layout_height="70dp" />

        <TextView
            android:layout_width="wrap_content"
            android:text="@string/connectToInternet"
            android:textSize="11sp"
            android:layout_height="wrap_content" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:gravity="center"
        android:visibility="gone"
        android:id="@+id/LoadingViews"
        android:orientation="vertical">

        <com.wang.avi.AVLoadingIndicatorView
            android:id="@+id/mainActivityAvi"
            style="@style/AVLoadingIndicatorView"
            android:layout_width="40dp"
            android:visibility="gone"
            android:layout_height="40dp"
            app:indicatorColor="@color/accent"
            app:indicatorName="LineScaleIndicator" />

        <ProgressBar
            android:id="@+id/progressBarSignUp"
            style="?android:attr/progressBarStyle"
            android:visibility="visible"
            android:layout_width="40dp"
            android:layout_height="40dp"/>

        <TextView
            android:id="@+id/loadingAdsMessage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Loading..."
            android:textSize="10sp" />

    </LinearLayout>

    <RelativeLayout
        android:id="@+id/blackBack"
        android:layout_width="match_parent"
        android:background="@color/black"
        android:alpha="0"
        android:visibility="gone"
        android:layout_height="match_parent" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:id="@+id/telemetryLayout"
        android:visibility="visible"
        android:layout_height="match_parent">

        <android.support.v7.widget.CardView
            android:id="@+id/card"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            app:cardElevation="1dp"
            app:cardCornerRadius="15dp"
            android:layout_margin="5dp">

            <RelativeLayout
                android:id="@+id/normalTelemetryLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:orientation="horizontal"
                        android:gravity="start"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/telemetryBack"
                            android:layout_width="40dp"
                            android:visibility="gone"
                            android:background="@drawable/ic_action_back"
                            android:layout_height="40dp" />

                        <TextView
                            android:id="@+id/viewingDateText"
                            android:layout_width="wrap_content"
                            android:text="Uploaded on: 17 Nov."
                            android:textSize="16sp"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="20dp"
                            android:gravity="center_vertical"
                            android:textColor="@color/colorPrimaryDark"
                            android:layout_height="match_parent" />


                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:background="@color/colorPrimaryDark"
                        android:layout_marginRight="15dp"
                        android:layout_marginLeft="15dp"
                        android:layout_height="1dp"/>

                    <com.bry.adcafe.models.LockableScrollView
                        android:id="@+id/ScrollView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:overScrollMode="never"
                        android:scrollbars="none">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:paddingLeft="20dp"
                                android:paddingRight="20dp"
                                android:paddingBottom="10dp"
                                android:paddingTop="10dp"
                                android:layout_height="wrap_content">

                                <RelativeLayout
                                    android:id="@+id/imageLayout"
                                    android:layout_width="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:visibility="visible"
                                    android:layout_height="wrap_content">

                                    <android.support.v7.widget.CardView
                                        android:layout_width="wrap_content"
                                        app:cardCornerRadius="10dp"
                                        android:layout_height="wrap_content">

                                        <ImageView
                                            android:id="@+id/adImageBackground"
                                            android:scaleType="centerCrop"
                                            android:layout_width="140dp"
                                            android:layout_height="160dp" />

                                        <ImageView
                                            android:id="@+id/adImage"
                                            android:scaleType="fitCenter"
                                            android:layout_width="140dp"
                                            android:layout_height="160dp" />

                                    </android.support.v7.widget.CardView>

                                </RelativeLayout>

                                <LinearLayout
                                    android:id="@+id/takeDownView"
                                    android:layout_width="wrap_content"
                                    android:orientation="vertical"
                                    android:layout_marginLeft="5dp"
                                    android:layout_height="wrap_content">

                                    <TextView
                                        android:id="@+id/uploaderText"
                                        android:layout_width="wrap_content"
                                        android:textSize="13sp"
                                        android:layout_marginLeft="10dp"
                                        android:layout_marginTop="15dp"
                                        android:text="Uploader: uploaderemail@gmail.com"
                                        android:layout_height="wrap_content" />

                                    <TextView
                                        android:id="@+id/statusText"
                                        android:layout_width="wrap_content"
                                        android:textSize="13sp"
                                        android:layout_marginLeft="10dp"
                                        android:layout_marginTop="10dp"
                                        android:text="Status: Uploaded."
                                        android:layout_height="wrap_content" />

                                    <android.support.v7.widget.CardView
                                        android:id="@+id/takeDownCard"
                                        android:visibility="visible"
                                        android:layout_width="match_parent"
                                        android:layout_gravity="bottom"
                                        app:cardBackgroundColor="@color/colorPrimaryDark"
                                        app:cardCornerRadius="10dp"
                                        app:cardElevation="0dp"
                                        android:layout_marginTop="20dp"
                                        android:layout_marginLeft="10dp"
                                        android:layout_marginRight="20dp"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:id="@+id/takeDownText"
                                            android:layout_width="match_parent"
                                            android:gravity="center"
                                            android:textColor="@color/white"
                                            android:layout_marginTop="13dp"
                                            android:layout_marginBottom="13dp"
                                            android:text="TAKE DOWN."
                                            android:textSize="11sp"
                                            android:layout_height="wrap_content" />

                                    </android.support.v7.widget.CardView>

                                </LinearLayout>


                            </LinearLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:paddingTop="10dp"
                                android:paddingRight="15dp"
                                android:paddingLeft="15dp">

                                <android.support.v7.widget.CardView
                                    android:layout_width="135dp"
                                    android:layout_height="140dp"
                                    android:layout_alignParentRight="true"
                                    app:cardBackgroundColor="@color/whiteish"
                                    android:layout_marginRight="15dp"
                                    app:cardCornerRadius="12dp"
                                    app:cardElevation="0dp">

                                    <TextView
                                        android:id="@+id/userSeenNumber"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:textColor="@color/colorPrimaryLight"
                                        android:text="94"
                                        android:textSize="40sp" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="top"
                                        android:layout_marginTop="5dp"
                                        android:text="Users Seen:"
                                        android:textAlignment="center"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="bottom"
                                        android:layout_marginBottom="5dp"
                                        android:text="SO FAR"
                                        android:textAlignment="center"
                                        android:textSize="11sp" />

                                </android.support.v7.widget.CardView>

                                <android.support.v7.widget.CardView
                                    android:layout_width="135dp"
                                    android:layout_height="140dp"
                                    android:layout_marginLeft="15dp"
                                    android:layout_alignParentLeft="true"
                                    app:cardBackgroundColor="@color/whiteish"
                                    app:cardCornerRadius="12dp"
                                    app:cardElevation="0dp">

                                    <TextView
                                        android:id="@+id/targetNumber"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:textColor="@color/colorPrimaryLight"
                                        android:text="N/A"
                                        android:textSize="40sp" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="top"
                                        android:layout_marginTop="5dp"
                                        android:text="Users Targeted:"
                                        android:textAlignment="center"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="bottom"
                                        android:layout_marginBottom="5dp"
                                        android:text="ON UPLOAD."
                                        android:textAlignment="center"
                                        android:textSize="11sp" />

                                </android.support.v7.widget.CardView>

                            </RelativeLayout>

                            <android.support.v7.widget.CardView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                app:cardBackgroundColor="@color/whiteish"
                                android:layout_marginRight="30dp"
                                android:layout_marginTop="20dp"
                                android:layout_marginBottom="10dp"
                                android:layout_marginLeft="30dp"
                                app:cardCornerRadius="12dp"
                                app:cardElevation="0dp">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:orientation="vertical"
                                    android:layout_height="match_parent">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:paddingLeft="20dp"
                                        android:paddingRight="20dp"
                                        android:paddingTop="10dp"
                                        android:paddingBottom="10dp"
                                        android:orientation="horizontal"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:id="@+id/amountPaidNumber"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimaryLight"
                                            android:text="2150.46 Ksh"
                                            android:textSize="25sp" />

                                        <TextView
                                            android:layout_marginLeft="20dp"
                                            android:layout_gravity="center_vertical"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="Total amount Paid."
                                            android:textSize="14sp" />

                                    </LinearLayout>

                                    <View
                                        android:layout_width="match_parent"
                                        android:background="@color/colorPrimaryDark"
                                        android:layout_marginLeft="20dp"
                                        android:layout_marginRight="20dp"
                                        android:layout_height="0.5dp"/>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:paddingLeft="20dp"
                                        android:paddingRight="20dp"
                                        android:paddingTop="10dp"
                                        android:paddingBottom="10dp"
                                        android:orientation="horizontal"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:id="@+id/reimbursalAmountNumber"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimaryLight"
                                            android:text="2150.46 Ksh"
                                            android:textSize="25sp" />

                                        <TextView
                                            android:layout_marginLeft="20dp"
                                            android:layout_gravity="center_vertical"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="Reimbursing Amount."
                                            android:textSize="14sp" />

                                    </LinearLayout>

                                    <android.support.v7.widget.CardView
                                        android:id="@+id/reimburseCard"
                                        android:visibility="visible"
                                        android:layout_width="match_parent"
                                        android:layout_gravity="bottom"
                                        app:cardBackgroundColor="@color/colorPrimaryDark"
                                        app:cardCornerRadius="10dp"
                                        app:cardElevation="0dp"
                                        android:layout_marginTop="10dp"
                                        android:layout_marginLeft="20dp"
                                        android:layout_marginRight="20dp"
                                        android:layout_marginBottom="20dp"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:gravity="center"
                                            android:textColor="@color/white"
                                            android:layout_marginTop="13dp"
                                            android:layout_marginBottom="13dp"
                                            android:text="REIMBURSE."
                                            android:textSize="11sp"
                                            android:layout_height="wrap_content" />

                                    </android.support.v7.widget.CardView>

                                </LinearLayout>

                            </android.support.v7.widget.CardView>

                            <android.support.v7.widget.CardView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                app:cardBackgroundColor="@color/whiteish"
                                android:layout_marginRight="30dp"
                                android:layout_marginTop="10dp"
                                android:layout_marginBottom="20dp"
                                android:layout_marginLeft="30dp"
                                app:cardCornerRadius="12dp"
                                app:cardElevation="0dp">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:orientation="vertical"
                                    android:layout_height="match_parent">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:paddingLeft="20dp"
                                        android:paddingRight="20dp"
                                        android:paddingTop="10dp"
                                        android:paddingBottom="10dp"
                                        android:orientation="horizontal"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:layout_gravity="center_vertical"
                                            android:layout_width="100dp"
                                            android:layout_height="wrap_content"
                                            android:text="Category:"
                                            android:textSize="17sp" />

                                        <TextView
                                            android:id="@+id/categoryText"
                                            android:layout_width="wrap_content"
                                            android:layout_gravity="center_vertical"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimaryLight"
                                            android:text="Women's clothing websites."
                                            android:textSize="13sp" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:paddingLeft="20dp"
                                        android:paddingRight="20dp"
                                        android:paddingTop="10dp"
                                        android:paddingBottom="10dp"
                                        android:orientation="horizontal"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:layout_gravity="center_vertical"
                                            android:layout_width="100dp"
                                            android:layout_height="wrap_content"
                                            android:text="Website Incentive:"
                                            android:textSize="15sp" />

                                        <TextView
                                            android:id="@+id/webclickIncentiveText"
                                            android:layout_width="wrap_content"
                                            android:layout_gravity="center_vertical"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimaryLight"
                                            android:text="5000Ksh (1000 * 5Ksh)."
                                            android:textSize="13sp" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:paddingLeft="20dp"
                                        android:paddingRight="20dp"
                                        android:paddingTop="10dp"
                                        android:paddingBottom="10dp"
                                        android:orientation="horizontal"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:layout_gravity="center_vertical"
                                            android:layout_width="100dp"
                                            android:layout_height="wrap_content"
                                            android:text="Number of Pins:"
                                            android:textSize="15sp" />

                                        <TextView
                                            android:id="@+id/numberOfPinsText"
                                            android:layout_width="wrap_content"
                                            android:layout_gravity="center_vertical"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimaryLight"
                                            android:text="50 Pins."
                                            android:textSize="13sp" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:paddingLeft="20dp"
                                        android:paddingRight="20dp"
                                        android:paddingTop="10dp"
                                        android:paddingBottom="10dp"
                                        android:orientation="horizontal"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:layout_gravity="center_vertical"
                                            android:layout_width="100dp"
                                            android:layout_height="wrap_content"
                                            android:text="Website:"
                                            android:textSize="17sp" />

                                        <TextView
                                            android:id="@+id/websiteText"
                                            android:layout_width="120dp"
                                            android:layout_gravity="center_vertical"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimaryLight"
                                            android:text="www.myco.org"
                                            android:textSize="13sp" />

                                        <RelativeLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent">

                                            <ImageButton
                                                android:id="@+id/editWebsiteIcon"
                                                android:layout_centerVertical="true"
                                                android:layout_alignParentRight="true"
                                                android:layout_width="20dp"
                                                android:background="@drawable/ic_action_edit"
                                                android:layout_height="20dp"/>

                                        </RelativeLayout>

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:paddingLeft="20dp"
                                        android:paddingRight="20dp"
                                        android:paddingTop="10dp"
                                        android:paddingBottom="10dp"
                                        android:orientation="horizontal"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:layout_gravity="center_vertical"
                                            android:layout_width="100dp"
                                            android:layout_height="wrap_content"
                                            android:text="Phone Number:"
                                            android:textSize="17sp" />

                                        <RelativeLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent">

                                            <TextView
                                                android:id="@+id/phoneNoText"
                                                android:layout_width="120dp"
                                                android:gravity="center_vertical"
                                                android:layout_height="match_parent"
                                                android:textColor="@color/colorPrimaryLight"
                                                android:text="07123 456 78"
                                                android:textSize="13sp" />

                                            <EditText
                                                android:id="@+id/phoneNoEditText"
                                                android:layout_width="120dp"
                                                android:visibility="gone"
                                                android:layout_height="wrap_content"
                                                android:layout_centerInParent="true"
                                                android:background="@android:color/white"
                                                android:hint="07123 456 78"
                                                android:inputType="phone"
                                                android:nextFocusDown="@+id/passwordEditText"
                                                android:paddingTop="10dp"
                                                android:paddingBottom="10dp"
                                                android:cursorVisible="true"
                                                android:textColor="@android:color/black"
                                                android:textColorHint="@color/colorAccent"
                                                android:textSize="12sp" />

                                        </RelativeLayout>

                                        <RelativeLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent">

                                            <ImageButton
                                                android:id="@+id/cancelEditingPhoneNo"
                                                android:layout_width="25dp"
                                                android:visibility="gone"
                                                android:layout_centerVertical="true"
                                                android:layout_alignParentRight="true"
                                                android:layout_marginRight="25dp"
                                                android:background="@drawable/ic_action_cancel"
                                                android:layout_height="25dp" />

                                            <ImageButton
                                                android:id="@+id/editPhoneNoIcon"
                                                android:layout_width="20dp"
                                                android:layout_centerVertical="true"
                                                android:layout_alignParentRight="true"
                                                android:background="@drawable/ic_action_edit"
                                                android:layout_height="20dp"/>

                                        </RelativeLayout>

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:paddingLeft="20dp"
                                        android:paddingRight="20dp"
                                        android:paddingTop="10dp"
                                        android:paddingBottom="10dp"
                                        android:orientation="horizontal"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:layout_gravity="center_vertical"
                                            android:layout_width="100dp"
                                            android:layout_height="wrap_content"
                                            android:text="Contact Locations:"
                                            android:textSize="15sp" />

                                        <TextView
                                            android:id="@+id/contactLocationText"
                                            android:layout_width="wrap_content"
                                            android:layout_gravity="center_vertical"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimaryLight"
                                            android:text="5 Locations."
                                            android:textSize="13sp" />

                                    </LinearLayout>

                                </LinearLayout>

                            </android.support.v7.widget.CardView>

                        </LinearLayout>

                    </com.bry.adcafe.models.LockableScrollView>

                </LinearLayout>

                <ProgressBar
                    android:id="@+id/loadingProgressBar"
                    android:layout_width="20dp"
                    android:layout_alignParentRight="true"
                    android:visibility="invisible"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_height="20dp"
                    android:layout_alignParentEnd="true" />

            </RelativeLayout>

            <ImageView
                android:id="@+id/statTelemetryIcon"
                android:layout_width="50dp"
                android:visibility="gone"
                android:background="@drawable/stats"
                android:layout_gravity="center"
                android:layout_height="45dp" />

        </android.support.v7.widget.CardView>

    </RelativeLayout>





    <View
        android:id="@+id/swipeBackView"
        android:layout_width="30dp"
        android:layout_gravity="bottom"
        android:layout_marginBottom="80dp"
        android:layout_height="180dp"/>

    <View
        android:id="@+id/swipeBackViewIndicator"
        android:layout_width="3dp"
        android:background="@color/colorPrimary"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="350dp"
        android:layout_height="0dp" />

    <RelativeLayout
        android:id="@+id/isGoingBackIndicator"
        android:layout_width="wrap_content"
        android:layout_marginTop="60dp"
        android:translationX="-40dp"
        android:background="@drawable/circle_design_timer_file"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="30dp"
            android:background="@drawable/ic_action_back"
            android:layout_height="30dp" />

    </RelativeLayout>

</android.support.design.widget.CoordinatorLayout>
